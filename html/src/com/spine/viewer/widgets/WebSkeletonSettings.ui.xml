<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:w='urn:import:com.projectqt.viewer.widgets'>
    <g:HTMLPanel>
        <details>
            <summary ui:field="title"></summary>
            <div>
                <w:Fieldset ui:field="skinFieldset" legend="Select a skin"></w:Fieldset>
                <w:Fieldset ui:field="animationFieldset" legend="Select an animation"></w:Fieldset>
                <w:Fieldset ui:field="slotFieldset" legend="Select slots">
                    <w:Details ui:field="slotsDetails" summary="All slots"></w:Details>
                </w:Fieldset>
            </div>
            <w:NumberInput ui:field="positionXSlider" label="Position X" value="0" step="1"></w:NumberInput>
            <w:NumberInput ui:field="positionYSlider" label="Position Y" value="0" step="1"></w:NumberInput>
            <w:NumberInput ui:field="scaleSlider" label="Scale" value="1" step="0.01"></w:NumberInput>
            <w:NumberInput ui:field="rotationSlider" label="Rotation" value="0" step="1"></w:NumberInput>
            <w:NumberInput ui:field="speedSlider" label="Speed" value="1" step="0.01"></w:NumberInput>
            <w:NumberInput ui:field="mixSlider" label="Mix" value="0.3" step="0.1"></w:NumberInput>
            <w:NumberInput ui:field="volumeSlider" label="Volume" value="1" step="0.01"></w:NumberInput>
            <g:Button ui:field="pauseButton">Pause</g:Button><g:CheckBox ui:field="loopCheckbox" value="true">Loop</g:CheckBox><br></br>
            <g:CheckBox ui:field="flipXCheckbox">Flip X</g:CheckBox><g:CheckBox ui:field="flipYCheckbox">Flip Y</g:CheckBox>
            <g:Button ui:field="bonesButton">Bones</g:Button><g:Button ui:field="slotsButton">Slots</g:Button><g:Button ui:field="bothButton">Both</g:Button><br></br>
            <g:CheckBox ui:field="premultipliedCheckbox" value="true">Premultiplied</g:CheckBox><br></br>
            <g:CheckBox ui:field="bonesCheckbox">Bones</g:CheckBox><br></br>
            <g:CheckBox ui:field="regionsCheckbox">Regions</g:CheckBox><br></br>
            <g:CheckBox ui:field="boundsCheckbox">Bounds</g:CheckBox><br></br>
            <g:CheckBox ui:field="meshHullCheckbox">Mesh Hull</g:CheckBox><br></br>
            <g:CheckBox ui:field="meshTrianglesCheckbox">Mesh Triangles</g:CheckBox><br></br>
            <w:ProgressBar ui:field="durationBar" text="Timeline" value="0" max="1"></w:ProgressBar>
        </details>
    </g:HTMLPanel>
</ui:UiBinder>